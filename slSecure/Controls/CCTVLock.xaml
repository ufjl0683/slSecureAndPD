<UserControl x:Class="slSecure.Controls.CCTVLock"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" MouseLeftButtonUp="UserControl_MouseLeftButtonUp">

    <Grid x:Name="LayoutRoot" Background="White" MouseLeftButtonDown="LayoutRoot_MouseLeftButtonDown" >
        <WebBrowser x:Name="browser"  Source="{Binding Url}" MouseLeftButtonDown="browser_MouseLeftButtonDown" Margin="0,28,0,0" />
        <Button Content="" Click="Button_Click" Height="28" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
